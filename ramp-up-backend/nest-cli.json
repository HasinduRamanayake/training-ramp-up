{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ramp-up-backend/src",
  "monorepo": true,
  "root": "apps/ramp-up-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/ramp-up-backend/tsconfig.app.json"
  },
  "projects": {
    "ramp-up-backend": {
      "type": "application",
      "root": "apps/ramp-up-backend",
      "entryFile": "main",
      "sourceRoot": "apps/ramp-up-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ramp-up-backend/tsconfig.app.json"
      }
    },
    "student-records": {
      "type": "application",
      "root": "apps/student-records",
      "entryFile": "main",
      "sourceRoot": "apps/student-records/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student-records/tsconfig.app.json"
      }
    },
    "file-uploader": {
      "type": "application",
      "root": "apps/file-uploader",
      "entryFile": "main",
      "sourceRoot": "apps/file-uploader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-uploader/tsconfig.app.json"
      }
    },
    "file-processor": {
      "type": "application",
      "root": "apps/file-processor",
      "entryFile": "main",
      "sourceRoot": "apps/file-processor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-processor/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    }
  }
}